<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custome="http://schemas.android.com/apk/res/com.example.sansheng"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#dfdfdf"
    android:orientation="vertical" >

    <include
        android:id="@+id/Head_Bar"
        layout="@layout/head_bar" />

    <com.view.BindItem
        android:id="@+id/Item_Tecent"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/Head_Bar"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_marginTop="20dip"
        custome:icon_src="@drawable/icon_tecent_weibo"
        custome:item_text="@string/tecetnt_weibo" >
    </com.view.BindItem>

    <com.view.BindItem
        android:id="@+id/Item_Sina"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/Item_Tecent"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        custome:icon_src="@drawable/icon_xinlang_weibo"
        custome:item_text="@string/sina_weibo" >
    </com.view.BindItem>

</RelativeLayout>